{
  "name": "hitfactor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "20.11.x"
  },
  "scripts": {
    "========================= Development Scripts ====================": "",
    "start": "docker-compose up -d && concurrently -c \"auto\" --names \"API,WEB\" \"npm run start-api\" \"npm run start-web\"",
    "quick": "docker-compose up -d && concurrently -c \"auto\" --names \"API,WEB\" \"npm run start-api-quick\" \"npm run start-web\"",
    "start-api-quick": "cd api && npm run quick",
    "start-api": "cd api && npm start",
    "start-web": "cd web && npm start",
    "test": "node --test",
    "====================== Production / Build Scripts ================": "",
    "prod": "cd api && npm run prod",
    "postinstall": "(cd api && npm install); (cd web && npm install && npm run build);",
    "========================== Worker Scripts ========================": "",
    "uploadsWorker": "NODE_OPTIONS='--max-old-space-size=512' node scripts/uploadsWorker.js",
    "========================= USPSA Import Scripts ============================": "",
    "import": "node scripts/import.js",
    "========================= Database Scripts ============================": "",
    "hydrate-quick": "QUICK_DEV=1 node scripts/hydrateMongo.js",
    "hydrate": "node scripts/hydrateMongo.js",
    "dump": "mongodump $MONGO_URL"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "zenrows": "^1.3.2"
  },
  "dependencies": {
    "lodash.uniqby": "^4.7.0",
    "undici": "^6.18.2",
    "uuid": "^9.0.1"
  }
}
