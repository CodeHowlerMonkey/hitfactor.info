{
  "name": "hitfactorinfo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "20.x.x"
  },
  "scripts": {
    "========================= Dev Scripts ====================": "",
    "start": "concurrently -c \"auto\" --names \"API,WEB\" \"npm run start-api\" \"npm run start-web\"",
    "test": "tsx --test",
    "====================== Production / Build Scripts ================": "",
    "prod": "cd api && npm run prod",
    "postinstall": "(cd api && npm install); (cd web && npm install && npm run build);",
    "========================== Upload Scripts (Render.com Worker/CRON) ========================": "",
    "uploadMatches": "NODE_OPTIONS='--max-old-space-size=512' node --import=tsx scripts/upload/matchesLoop.ts",
    "uploadScores": "NODE_OPTIONS='--max-old-space-size=512' node --import=tsx scripts/upload/scoresLoop.ts",
    "uploadMeta": "NODE_OPTIONS='--max-old-space-size=2048' node --import=tsx scripts/upload/statsMetaLoop.ts",
    "members": "node --import=tsx scripts/activeMembers.ts",
    "========================= DB Admin Scripts ============================": "",
    "dump": "mongodump $MONGO_URL",
    "===================== Internal (Don't Run Directly) ======================": "",
    "start-api": "cd api && npm start",
    "start-web": "cd web && npm start",
    "========================= Legacy / Broken Scripts ============================": "",
    "start-api-local": "cd api && npm run local",
    "local": "docker-compose up -d && concurrently -c \"auto\" --names \"API,WEB\" \"npm run start-api-local\" \"npm run start-web\"",
    "import": "node scripts/import.js",
    "hydrate-local": "LOCAL_DEV=1 node scripts/hydrateMongo.js",
    "hydrate": "node scripts/hydrateMongo.js"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.51.15",
    "@types/lodash": "^4.17.9",
    "@types/node": "^22.7.4",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.5",
    "prettier": "^3.3.3",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "zenrows": "^1.3.2"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.637.0",
    "lodash.throttle": "^4.1.1",
    "lodash.uniqby": "^4.7.0",
    "simple-statistics": "^7.8.7",
    "undici": "^6.18.2",
    "uuid": "^9.0.1"
  }
}
