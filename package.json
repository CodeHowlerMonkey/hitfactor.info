{
  "name": "hitfactor",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "20.11.x"
  },
  "scripts": {
    "======================== Development Scripts ====================": "",
    "start": "concurrently -c \"auto\" --names \"API,WEB\" \"npm run start-api\" \"npm run start-web\"",
    "start-api": "cd api && npm start",
    "start-web": "cd web && npm start",
    "====================== Production / Build Scripts ================": "",
    "prod": "cd api && npm run prod",
    "postinstall": "(cd api && npm install); (cd web && npm install && npm run build);",
    "========================= Import Scripts ============================": "",
    "import-meta": "node scripts/importMeta.js"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "zenrows": "^1.3.2"
  }
}
